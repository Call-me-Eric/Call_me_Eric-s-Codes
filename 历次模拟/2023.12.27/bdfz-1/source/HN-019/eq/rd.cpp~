#include<iostream>
#include<cstdio>
#define mod 998244353
using namespace std;
int n=700,q=700;
int main()
{
	freopen("eq.in","w",stdout);
	int x;
	cout<<n<<' '<<q<<endl;
	for (int i=1;i<=n;++i)
	{
		for (int j=1;j<=n+1;++j) cout<<RAND()%mod<<' ';
		cout<<endl;
	}
	while (q--)
	{
		x=RAND()%n+1;
		for (int i=1;i<=n+1;++i) cout<<RAND()%mod<<' ';
		cout<<endl;
	}
	return 0;
}
